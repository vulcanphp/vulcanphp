{
    "name": "vulcanphp/vulcanphp",
    "type": "project",
    "description": "Vulcan PHP is a MVC based micro framework to create PHP Applications easily",
    "keywords": ["vulcanphp", "mvc"],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "vulcanphp/sweetview": "^1.0",
        "vulcanphp/prettyerror": "^1.0",
        "vulcanphp/phprouter": "^1.0",
        "vulcanphp/fastcache": "^1.0",
        "vulcanphp/simpledb": "^1.0",
        "vulcanphp/translator": "^1.0",
        "vulcanphp/filesystem": "^1.0",
        "vulcanphp/easycurl": "^1.0",
        "vulcanphp/core": "^1.0"
    },
    "authors": [
        {
            "name": "Shahin Moyshan",
            "email": "freelance.shain@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "./app"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
